    <style>
        /* Định dạng cho toàn bộ thanh điều hướng */
        #wide-nav {
            background-color: #fff; /* Màu nền của thanh điều hướng */
            border-bottom: 1px solid #ddd; /* Đường viền dưới của thanh điều hướng */
            padding: 10px 0; /* Khoảng cách trên và dưới của thanh điều hướng */
        }

            /* Định dạng cho các mục điều hướng */
            #wide-nav .nav {
                list-style: none; /* Loại bỏ dấu chấm đầu dòng của danh sách */
                margin: 0;
                padding: 0;
                display: flex; /* Hiển thị các mục điều hướng theo hàng ngang */
            }

                /* Định dạng cho các liên kết trong thanh điều hướng */
                #wide-nav .nav a {
                    text-decoration: none; /* Loại bỏ gạch chân của liên kết */
                    color: #333; /* Màu chữ của liên kết */
                    padding: 10px 15px; /* Khoảng cách xung quanh liên kết */
                    display: block; /* Hiển thị liên kết dưới dạng khối */
                    transition: color 0.3s; /* Hiệu ứng chuyển màu chữ khi hover */
                }

                    /* Định dạng cho liên kết khi di chuột qua */
                    #wide-nav .nav a:hover {
                        color: #007bff; /* Màu chữ khi hover */
                        background-color: #f8f9fa; /* Màu nền khi hover */
                        border-radius: 5px; /* Bo góc liên kết khi hover */
                    }

                /* Định dạng cho các menu con (dropdown) */
                #wide-nav .nav .sub-menu {
                    display: none; /* Ẩn các menu con mặc định */
                    position: absolute; /* Định vị menu con so với phần tử cha */
                    background-color: #fff; /* Màu nền của menu con */
                    border: 1px solid #ddd; /* Đường viền của menu con */
                    padding: 10px 0; /* Khoảng cách trên và dưới của menu con */
                    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Đổ bóng cho menu con */
                    z-index: 1000; /* Đảm bảo menu con nằm trên các phần tử khác */
                }

                /* Hiển thị menu con khi hover lên mục có menu con */
                #wide-nav .nav .menu-item-has-children:hover .sub-menu {
                    display: block; /* Hiển thị menu con khi hover */
                }

                /* Định dạng cho các liên kết trong menu con */
                #wide-nav .nav .sub-menu a {
                    padding: 10px 20px; /* Khoảng cách xung quanh liên kết trong menu con */
                    color: #333; /* Màu chữ của liên kết trong menu con */
                }

                    /* Định dạng cho liên kết trong menu con khi hover */
                    #wide-nav .nav .sub-menu a:hover {
                        color: #007bff; /* Màu chữ khi hover trong menu con */
                        background-color: #f8f9fa; /* Màu nền khi hover trong menu con */
                    }

        /* Định dạng cho các nút đăng ký lái thử */
        .header-button .button {
            background-color: #28a745; /* Màu nền của nút */
            color: #fff; /* Màu chữ của nút */
            border: none; /* Loại bỏ đường viền của nút */
            padding: 10px 20px; /* Khoảng cách xung quanh chữ trong nút */
            border-radius: 50px; /* Bo góc nút */
            text-align: center; /* Canh giữa chữ trong nút */
            font-size: 14px; /* Kích thước chữ trong nút */
            transition: background-color 0.3s; /* Hiệu ứng chuyển màu nền khi hover */
        }

            /* Định dạng cho nút khi hover */
            .header-button .button:hover {
                background-color: #218838; /* Màu nền khi hover */
            }

        .carousel-inner img {
            width: 100%;
